<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctgu.proname.dao.SchoolTypeDao">
	<select id="querySchoolType"
		resultType="com.ctgu.proname.entity.SchoolType">
		SELECT id,explains,datetime FROM school_type
	</select>

	<insert id="insertSchoolType" useGeneratedKeys="true"
		keyProperty="id">
		INSERT INTO school_type values(null,#{explains},null)
		</insert>
</mapper>